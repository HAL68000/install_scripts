        "install_script": "scripts/nextjs-ollama-llm-ui/arm/install.sh",
        "uninstall_script": "scripts/nextjs-ollama-llm-ui/arm/uninstall.sh",
        "start_command": "cd $HOME/nextjs-ollama-llm-ui && npm run dev",
