        "post_installation_script": "scripts/ssh/arm/post_install.sh",
        "icon": "scripts/ssh/icon.png"
    },
    {
        "name": "ollama",
        "description": "local LLMs made easy",
        "install_script": "scripts/ollama/arm/install.sh",
