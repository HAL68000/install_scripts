        "stop_command": "pkill -f 'ollama serve'",
        "post_installation_script": "scripts/ollama/arm/post_install.sh",
        "icon": "scripts/ollama/icon.png"
    },
    {
        "name": "ubuntu",
        "description": "Ubuntu distro on Termux",
        "install_script": "scripts/ubuntu/arm/install.sh",
        "uninstall_script": "scripts/ubuntu/arm/uninstall.sh",
