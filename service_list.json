        "install_script": "scripts/mosquitto/arm/install.sh",
        "uninstall_script": "scripts/mosquitto/arm/uninstall.sh",
        "start_command": "mosquitto -c $PREFIX/etc/mosquitto/mosquitto.conf",
        "stop_command": "pkill -f mosquitto",
